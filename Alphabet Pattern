import java.util.*;
public class AlphbetPattern {
    public static void main(String args[]){
        int c;
        c=65;
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter n value:");
        int i,j,n;
        n=sc.nextInt();
        for(i=0;i<n;i++){
            c=(char)65;
            for(j=0;j<=i;j++){
                char c1=(char)(c+j);
                System.out.print(c1);
            }
            System.out.println();
        }
    }
}
