import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);

        // write ur code here
        int n,i,j,k=1;
        n=scn.nextInt();
        for(i=1;i<=n;i++){
            for(j=1;j<=i;j++){
                System.out.print(k+"\t");
                k++;
            }
            System.out.println();
        }

    }
}
