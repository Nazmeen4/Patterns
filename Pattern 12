import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);

        // write ur code here
        int n,i,j,a=0,b=1,c=1;
        n=scn.nextInt();
        for(i=0;i<n;i++){
            for(j=0;j<=i;j++){
                System.out.print(a+"\t");
                c=a+b;
                a=b;
                b=c;
            }
            System.out.println();
        }

    }
}
