import java.util.*;
public class Main{
    public static void main(String[] args){
        Scanner scn = new Scanner(System.in);
        int n,i,j,a,b,c;
        //write your code here
        n=scn.nextInt();
        for(i=0;i<n;i++){
            a=1;
            for(j=0;j<=i;j++){
                System.out.print(a+"\t");
                b=a*(i-j)/(j+1);
                a=b;
            }
            System.out.println();
        }
        

    }
}
