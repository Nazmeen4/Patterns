import java.util.*;

public class Main {

    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);

        // write ur code here
        int n=scn.nextInt();
        int i,j,k;
        for(i=0;i<n;i++){
            for(k=0;k<i;k++)
                System.out.print("\t");
            for(j=k;j<n;j++)
                System.out.print("*\t");
            System.out.println();
        }

    }
}
