public class Rhombus {
    public static void main(String args[]){
        int i,j,sp=5;
        for(i=0;i<5;i++){
            for(j=0;j<sp;j++)
                System.out.print(" ");
            for(j=0;j<5;j++)
                System.out.print("*"+"");
            System.out.println();
            sp--;
        }
    }
}
